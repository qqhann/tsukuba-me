{
  "name": "tsukuba-me",
  "productName": "TSUKUBA-ME",
  "version": "1.0.0",
  "description": "University of Tsukuba school life utilities. Manaba attend (atmnb) helper, and PaperCut U.T. uploader.",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "rebuild": "electron-rebuild -f -w keytar"
  },
  "build": {
    "asar": true,
    "asarUnpack": "node_modules/puppeteer/.local-chromium/**/*",
    "appId": "io.github.tsukuba-me",
    "mac": {
      "category": "public.app-category.productivity"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qqhann/tsukuba-me.git"
  },
  "keywords": [
    "Tsukuba",
    "University",
    "UniversityOfTsukuba",
    "TsukubaUniversity",
    "U.T.",
    "UT",
    "Manaba",
    "atmnb",
    "PaperCut",
    "zengaku",
    "zengakuPrinter"
  ],
  "author": "qqhann <qiu.gits@gmail.com> (https://qqhann.dev)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qqhann/tsukuba-me/issues"
  },
  "homepage": "https://github.com/qqhann/tsukuba-me#readme",
  "devDependencies": {
    "electron": "^5.0.2",
    "electron-builder": "^20.41.0",
    "electron-rebuild": "^1.8.5"
  },
  "dependencies": {
    "electron-store": "^3.2.0",
    "electron-util": "^0.11.0",
    "keytar": "^4.9.0",
    "materialize-css": "^1.0.0",
    "puppeteer": "^1.17.0",
    "puppeteer-core": "^1.17.0"
  }
}
